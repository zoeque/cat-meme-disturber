<Window x:Class="CatMemeDisturber.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CatMemeDisturber"
        mc:Ignorable="d"
        Title="CatMemeButton" Height="225" Width="200">
	<Grid Margin="0,0,0,9" HorizontalAlignment="Center" Width="200">
		<Button Content="HappyCat" HorizontalAlignment="Left" Margin="34,3,0,158" Width="128" RenderTransformOrigin="0.5,0.5" Click="HappyCat_Button_Click">
			<Button.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform AngleX="-0.674"/>
					<RotateTransform/>
					<TranslateTransform X="-0.388"/>
				</TransformGroup>
			</Button.RenderTransform>
		</Button>
		<Button Content="Goat" HorizontalAlignment="Left" Margin="34,98,0,63" Width="128" RenderTransformOrigin="0.5,0.5" Click="Goat_Button_Click">
