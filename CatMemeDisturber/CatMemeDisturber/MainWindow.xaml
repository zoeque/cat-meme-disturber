				</TransformGroup>
			</Button.RenderTransform>
		</Button>
		<Button Content="Maxwell" HorizontalAlignment="Left" Margin="34,50,0,111" Width="128" RenderTransformOrigin="0.5,0.5" Click="Maxwell_Button_Click">
			<Button.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform AngleX="-0.674"/>
					<RotateTransform/>
					<TranslateTransform X="-0.388"/>
				</TransformGroup>
			</Button.RenderTransform>
		</Button>
		<Button Content="Oiia" HorizontalAlignment="Left" Margin="34,146,0,15" Width="128" RenderTransformOrigin="0.5,0.5" Click="Oiia_Button_Click">
			<Button.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform AngleX="-0.674"/>
					<RotateTransform/>
					<TranslateTransform X="-0.388"/>
				</TransformGroup>
			</Button.RenderTransform>
		</Button>

	</Grid>
</Window>
